  "description": "Genomix Application WebServer Node",
  "main": "index.js",
  "scripts": {
    "test": "GEN_WS_DISABLE_REQUEST_LIMITS=true mocha",
    "db:create": "node ./scripts/create-database.js",
    "db:reset": "npm run db:create; npm run defaults:import",
    "defaults:import": "node ./scripts/import-defaults.js",
    "start": "node index.js",
    "start-server": "cd frontend;npm run build;cd ../;node ./index.js",
    "defaults:generate": "node ./defaults/build-initial-data.js",
    "client": "node test-client/index.js",
    "user:add": "node ./scripts/add-user.js",
    "frontend": "cd frontend; npm start",
    "update": "cd scripts; bash amazon-update.sh",
    "build:webserver": "cd scripts/build; ./build.sh",
    "build:frontend": "cd frontend/; npm run build",
    "build": "npm run build:frontend; npm run build:webserver"
