{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "ws-ui-schema/base_json/sample#",
	"type": "object",
	"properties": {
		"id": {
			"type": "string",
			"description": "UUID, the unique identifier for the sample"
		},
		"genotypeName": {
			"type": ["string", "null"],
			"description": "Grnptypr name"
		},
		"fileName": {
			"type": "string",
			"description": "Name of the file as it was during upload"
		},
		"hash": {
			"type": ["string","null"]
		},
		"type": {
			"type": "string",
			"description": "Standard - available for demo-user, advanced - only for registered.",
			"enum": [
				"standard",
				"advanced",
				"user",
				"default"
			]
		},
		"metadata": {
			"$ref": "sample_metadata#"
		},
		"vcf_fields": {
			"type": "array",
			"items": {
				"$ref": "vcf_field#"
			}
		},
		"is_analyzed": {
			"type": "boolean"
		},
		"analyzed_timestamp": {
			"type": "string",
			"format": "date-time"
		},
		"is_deleted": {
			"type": "boolean"
		},
		"commentaries": {
			"type": "array",
			"items": {
				"$ref": "comment#"
			}
		}
	},
	"required": [
		"id",
		"genotypeName",
		"fileName"
	]
}
