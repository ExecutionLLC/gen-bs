{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "ws-ui-schema/base_json/query_history#",
  "type": "object",
  "properties": {
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "description": {
      "type": "string"
    },
    "sample": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "view": {
      "$ref": "../base_json/view#"
    },
    "filters": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "../base_json/filter#"
      }
    },
    "fields_metadata": {
      "type": "array",
      "items": {
        "$ref": "../base_json/field_metadata#"
      }
    }
  },
  "required": [
    "sample",
    "view",
    "filters",
    "keywords"
  ]
}