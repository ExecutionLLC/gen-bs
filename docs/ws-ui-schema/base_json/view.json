{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "ws-ui-schema/base_json/view#",
  "title": "One view",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "UUID, the unique identifier for the view"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "description": "Localized view description for UI",
      "type": "string"
    },
    "type": {
      "type": "string",
      "description": "Standard - available for demo-user, advanced - only for registered.",
      "enum": [
        "standard",
        "advanced",
        "user"
      ]
    },
    "isCopyDisabled": {
      "type": "boolean",
      "description": "If true, user should not be allowed to copy the view on UI."
    },
    "modified_timestamp": {
      "type": "integer",
      "description": "Timestamp of the last view modification."
    },
    "viewListItems": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "view_list_item#"
      }
    }
  },
  "required": [
    "id",
    "name",
    "type",
    "viewListItems",
    "isCopyDisabled"
  ]
}