{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "ws-ui-schema/UI-WS/save_file#",
	"type": "object",
	"properties": {
		"file_name": {
			"type": "string"
		},
		"filters": {
			"type": "array",
			"minItems": 1,
			"items": {
				"$ref": "../base_json/filter#"
			}
		},
		"view": {
			"$ref": "../base_json/view#"
		},
		"url": {
			"type": "string",
			"format": "uri"
		},
		"description": {
			"type": "string"
		},
		"start_number": {
			"type": "integer"
		},
		"expected_number": {
			"type": "integer"
		},
		"vcf_link": {
			"type": "object",
			"properties": {
				"id": {
					"type": "number"
				},
				"name": {
					"type": "string"
				}
			},
			"required": [
				"id",
				"name"
			]
		}
	},
	"required": [
		"file_name",
		"filters",
		"view",
		"url",
		"vcf_link"
	]
}
