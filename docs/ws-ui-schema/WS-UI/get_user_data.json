{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id":"ws-ui-schema/WS-UI/get_user_data#",
	"title": "User",
	"type": "object",
	"properties": {
		"profileMetadata": {
			"$ref": "../base_json/user_metadata#"
		},
		"samples": {
			"type": "array",
			"items": {
				"$ref": "../base_json/sample_metadata#"
			}
		},
		"filters": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/filter#"
			}
		},
		"views": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/view#"
			}
		},
		"savedFiles": {
			"type": "array",
			"items": {
				"$ref": "../UI-WS/save_file#"
			}
		},
		"queriesHistory": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/query_history#"
			}
		},
		"totalFields":{
			"type": "array",
			"items": {
				"$ref": "../base_json/field_metadata#"
			}
		},
		"activeOperations": {
			"type": "array",
			"description": "Array of active operations for the session specified in the request header.",
			"items": {
				"type": "object",
				"properties": {
					"id": {
						"type": "string",
						"description": "Operation identifier"
					},
					"type": {
						"type": "string",
						"description": "Operation type.",
						"enum": [
							"search",
							"upload"
						]
					}
				}
			}
		}
	},
	"required": [
		"profileMetadata",
		"views",
		"filters",
		"samples",
		"queryHistory",
		"savedFiles",
		"totalFields",
		"activeOperations"
	]
}
