{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "One view",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "description": "Localized filter description for UI",
      "type": "string"
    },
    "type": {
      "type": "string",
      "description": "Standard - available for demo-user, advanced - only for registered.",
      "enum": [
        "standard",
        "advanced",
        "user"
      ]
    },
    "items": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "./view_list_item.json#"
      }
    }
  },
  "required": [
    "name",
    "type",
    "items"
  ]
}