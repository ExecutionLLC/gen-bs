{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"timestamp": {
			"type": "string",
			"format": "date-time"
		},
		"id": {
			"type": "number"
		},
		"hash": {
			"type": "string"
		},
		"vcf_fields": {
			"type": "array",
			"items": {
				"$ref": "../base_json/field_metadata.json#"
			}
		},
		"is_analysed": {
			"type": "boolean"
		},
		"is_default": {
			"type": "boolean"
		},
		"is_advanced": {
			"type": "boolean"
		}
	},
	"required": [
		"id",
		"hash",
		"is_analysed"
	]
}
