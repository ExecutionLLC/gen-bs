{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"modifiedTimestamp": {
			"type": "string",
			"description": "Timestamp indicating the time this exact version has been created."
		},
		"id": {
			"type": "string"
		},
		"file_name": {
			"type": "string",
			"description": "Name of the file as it was during upload"
		},
		"hash": {
			"type": "string"
		},
		"fields": {
			"type": "array",
			"items": {
				"$ref": "../base_json/field_metadata.json#"
			}
		},
		"sample_type": {
			"type": "string",
			"enum": [
				"standard",
				"advanced",
				"user"
			]
		},
		"is_analyzed": {
			"type": "boolean"
		},
		"analyzed_timestamp": {
			"type": "string"
		}
	},
	"required": [
		"id",
		"hash",
		"is_analyzed",
		"is_standard",
		"is_advanced"
	]
}