{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"modifiedTimestamp": {
			"type": "string",
			"format": "date-time"
		},
		"id": {
			"type": "string"
		},
		"hash": {
			"type": "string"
		},
		"vcf_fields": {
			"type": "array",
			"items": {
				"$ref": "../base_json/field_metadata.json#"
			}
		},
		"sample_type": {
			"type": "string",
			"enum": [
				"standard",
				"advanced",
				"user"
			]
		},
		"is_analysed": {
			"type": "boolean"
		}
	},
	"required": [
		"id",
		"hash",
		"is_analysed",
		"is_standard",
		"is_advanced"
	]
}