{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Vcf Filed Name",
	"type": "object",
	"properties": {
		"id": {
			"description": "The unique identifier for a field",
			"type": "number"
		},
		"name": {
			"description": "Text Label",
			"type": "string"
		},
		"description": {
			"description": "Text description for tooltip",
			"type": "string"
		},
		"group": {
			"type": "string"
		},
		"keywords": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/keyword.json#"
			}
		},
		"operators": {
			"type": "array",
			"items": {
				"type": "string",
				"enum": [
					"equal",
					"not equal",
					"in",
					"not in",
					"less",
					"less or equal",
					"greater",
					"greater or equal",
					"between",
					"not between",
					"is null",
					"is not null"
				]
			}
		},
		"filter_control_enable": {
			"type": "boolean"
		},
		"value_type": {
			"type": "string",
			"enum": [
				"float",
				"integer",
				"char",
				"string",
				"boolean"
			]
		},
		"editable": {
			"type": "boolean"
		},
		"tech": {
			"type": "boolean"
		},
		"available_values": {
			"type": "object",
			"properties": {
				"available_items": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "object",
						"properties": {
							"id": {
								"type": "number"
							},
							"name": {
								"type": "string"
							}
						}
					}
				},
				"selected_items": {
					"type": "array",
					"items": {
						"type": "integer"
					}
				},
				"multi_select": {
					"type": "boolean"
				}
			}
		}
	},
	"required": [
		"id",
		"name",
		"description",
		"filter_control_enable",
		"editable",
		"tech"
	]
}