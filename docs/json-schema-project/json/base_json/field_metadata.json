{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Vcf Filed Name",
	"type": "object",
	"properties": {
		"id": {
			"description": "The unique identifier for a field",
			"type": "string"
		},
		"name": {
			"description": "Text Label",
			"type": "string"
		},
		"label": {
			"description": "Display text for the field",
			"type": "string"
		},
		"source_type": {
			"type": "string",
			"description": "Where is the field came from",
			"enum": [
				"sample",
				"source"
			]
		},
		"source_name": {
			"type": "string",
			"description": "Name of the source. Should be equal to id for user-uploaded samples."
		},
		"value_type": {
			"type": "string",
			"description": "Value type which is got from the VCF file.",
			"enum": [
				"float",
				"integer",
				"char",
				"string",
				"boolean"
			]
		},
		"is_mandatory": {
			"type": "boolean"
		},
		"editable": {
			"type": "boolean"
		},
		"description": {
			"description": "Text description for tooltip",
			"type": "string"
		},
		"keywords": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/keyword.json#"
			}
		},
		"available_values": {
			"type": "array",
			"description": "Values for enum-like fields",

			"items": {
				"type": "object",
				"properties": {
					"id": {
						"type": "string"
					},
					"value": {
						"type": "string"
					}
				}
			}
		},
		"value": {
			"type": "string",
			"description": "Used when requesting update of an editable sample field."
		}
	},
	"required": [
		"id",
		"name",
		"label",
		"description",
		"filter_control_enable",
		"editable",
		"tech"
	]
}