{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "One filter",
	"type": "object",
	"properties": {
		"id": {
			"type": "string",
			"description": "UUID, the unique identifier for the filter"
		},
		"created": {
			"type": "timestamp"
		},
		"creator": {
			"type": "string"
		},
		"filter_id": {
			"type": "string",
			"description": "UUID, the unique identifier for the filter info(created, creator, and is deleted)"
		},
		"isDeleted": {
			"type": "boolean"
		},
		"type": {
			"type": "string",
			"description": "standard/default - available for demo-user, user - created by user, default - default filters for analyses",
			"enum": [
				"standard",
				"default",
				"user"
			]
		},
		"text": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"type": "object",
				"properties":{
					"languageId": {
						"type":"string"
					},
					"name": {
						"description": "Display text for the field",
						"type": "string"
					},
					"description": {
						"description": "Text description for tooltip",
						"type": "string"
					}
				}
			}
		},
		"rules": {
			"type": "string",
			"description": "A JSON object with rules needed by the UI for this filter."
		}
	},
	"required": [
		"id",
		"name",
		"type",
		"rules",
		"isDeleted"
	]
}