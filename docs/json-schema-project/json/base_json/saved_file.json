{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "One saved file",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "UUID, the unique identifier for the saved file"
    },
    "name": {
      "type": "string"
    },
    "timestamp": {
      "type": "timestamp"
    },
    "total_results": {
      "type": "integer"
    },
    "analysis": {
      "type": "object",
      "properties":{
        "id": {
          "type":"string"
        },
        "text": {
          "type": "array",
          "minItems": 1,
          "uniqueItems": true,
          "items": {
            "type": "object",
            "properties":{
              "languageId": {
                "type":"string"
              },
              "name": {
                "description": "Display name of the analysis",
                "type": "string"
              },
              "description": {
                "description": "Text description of the analysis",
                "type": "string"
              }
            }
          }
        }
      }
    },
    "filter": {
      "type": "object",
      "properties":{
        "id": {
          "type":"string"
        },
        "text": {
          "type": "array",
          "minItems": 1,
          "uniqueItems": true,
          "items": {
            "type": "object",
            "properties":{
              "languageId": {
                "type":"string"
              },
              "name": {
                "description": "Display name of the filter",
                "type": "string"
              },
              "description": {
                "description": "Text description of the filter",
                "type": "string"
              }
            }
          }
        }
      }
    },
    "model": {
      "type": "object",
      "properties":{
        "id": {
          "type":"string"
        },
        "text": {
          "type": "array",
          "minItems": 1,
          "uniqueItems": true,
          "items": {
            "type": "object",
            "properties":{
              "languageId": {
                "type":"string"
              },
              "name": {
                "description": "Display name of the model",
                "type": "string"
              },
              "description": {
                "description": "Text description of the model",
                "type": "string"
              }
            }
          }
        }
      }
    },
    "view": {
      "type": "object",
      "properties":{
        "id": {
          "type":"string"
        },
        "text": {
          "type": "array",
          "minItems": 1,
          "uniqueItems": true,
          "items": {
            "type": "object",
            "properties":{
              "languageId": {
                "type":"string"
              },
              "name": {
                "description": "Display name of the view",
                "type": "string"
              },
              "description": {
                "description": "Text description of the view",
                "type": "string"
              }
            }
          }
        }
      }
    },
    "samples": {
      "type": "array",
      "items": {
        "type": "object",
        "properties":{
          "id": {
            "type":"string"
          },
          "fileName": {
            "type":"string"
          },
          "genotypeName": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "single",
              "tumor",
              "normal",
              "proband",
              "mother",
              "father"
            ]
          }
        }
      }
    }
  },
  "required": [
    "id",
    "name",
    "type",
    "view_list_items",
    "is_copy_disabled"
  ]
}