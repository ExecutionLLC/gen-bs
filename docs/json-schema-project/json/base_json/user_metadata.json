{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "User",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"last_name": {
			"type": "string"
		},
		"email": {
			"type": "string",
			"format": "email"
		},
		"speciality": {
			"type": "string"
		},
		"default_langu_id": {
			"type": "string",
			"maxLength": 2
		},
		"number_paid_samples": {
			"type": "integer"
		}
	}
}
