{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "One view",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for a view",
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "description": "Localized filter description for UI",
      "type": "string"
    },
    "view_type": {
      "type": "string",
      "description": "Standard - available for demo-user, advanced - only for registered.",
      "enum": [
        "standard",
        "advanced",
        "user"
      ]
    },
    "is_copy_disabled": {
      "type": "boolean",
      "description": "If true, user should not be allowed to copy the view on UI."
    },
    "modifiedTimestamp": {
      "type": "integer",
      "description": "Timestamp of the last view modification."
    },
    "view_list_items": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "./view_list_item.json#"
      }
    }
  },
  "required": [
    "id",
    "name",
    "view_type",
    "view_list_items",
    "is_disabled_4copy"
  ]
}