{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "One filter",
	"type": "object",
	"properties": {
		"id": {
			"type": "number"
		},
		"name": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"rules": {
			"type": "string"
		},
		"is_standard": {
			"type": "boolean"
		},
		"is_advanced": {
			"type": "boolean"
		},
		"is_copy_disabled": {
			"type": "boolean"
		}
	},
	"required": [
		"id",
		"name",
		"rules",
		"is_standard"
	]
}
