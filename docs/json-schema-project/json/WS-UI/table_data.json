{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Main table data answer",
  "type": "array",
  "items": {
    "type": "object",
    "description": "Object representing table row.",
    "properties": {
      "field_id": {
        "type": "string",
        "description": "UUID of the field, either from sample or source."
      },
      "value": {
        "type": "string",
        "description": "Value of the field for the row."
      }
    }
  }
}