{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "User",
	"type": "object",
	"properties": {
		"id": {
			"type": "number"
		},
		"profile_metadata": {
			"$ref": "../base_json/user_metadata.json#"
		},
		"samples": {
			"type": "array",
			"items": {
				"$ref": "../base_json/sample_metadata.json#"
			}
		},
		"mutation_comments": {
			"type": "array",
			"items": {
				"$ref": "../base_json/commentary.json#"
			}
		},
		"filters": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/filter.json#"
			}
		},
		"views": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/view.json#"
			}
		},
		"saved_files": {
			"type": "array",
			"items": {
				"$ref": "../WS_AS/save_file.json#"
			}
		},
		"queries_history": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/query_history.json#"
			}
		},
		"number_paid_samples": {
			"type": "integer"
		}
	},
	"required": [
		"id",
		"number_paid_samples"
	]
}
