{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"timestamp": {
			"type": "string",
			"format": "date-time"
		},
		"description": {
			"type": "string"
		},
		"curent_sample": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer"
				},
				"name": {
					"type": "string"
				}
			}
		},
		"current_view": {
			"$ref": "../base_json/view.json#"
		},
		"current_filters": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/filter.json#"
			}
		},
		"current_keywords": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/keyword.json#"
			}
		},
		"current_fields_metadata": {
			"type": "array",
			"items": {
				"$ref": "../base_json/field_metadata.json#"
			}
		},
		"start_number": {
			"type": "integer"
		},
		"total_number": {
			"type": "integer"
		}
	},
	"required": [
		"curent_sample",
		"current_view",
		"current_filters",
		"current_keywords"
	]
}
