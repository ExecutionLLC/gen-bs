{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"curent_sample": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer"
				},
				"name": {
					"type": "string"
				}
			}
		},
		"current_view": {
			"$ref": "../base_json/view.json#"
		},
		"current_filters": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/filter.json#"
			}
		},
		"current_search_value": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"type": "string"
			}
		},
		"current_field_controls": {
			"type": "array",
			"minItems": 1,
			"uniqueItems": true,
			"items": {
				"$ref": "../base_json/keyword.json#"
			}
		},
		"expected_number": {
			"type": "integer"
		},
		"start_number": {
			"type": "integer"
		}
	},
	"required": [
		"curent_sample",
		"current_view",
		"current_filters",
		"current_search_value",
		"current_field_controls"
	]
}
