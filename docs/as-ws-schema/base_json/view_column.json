{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "One column in view",
  "type": "object",
  "properties": {
    "source_name": {
      "type": "string",
      "description": "Name of the source file. If source_name is empty, then current column is column of the sample file."
    },
    "column_name": {
      "type": "string",
      "description": "Name of the column."
    }
  },
  "required": [
    "source_name",
    "column_name"
  ]
}